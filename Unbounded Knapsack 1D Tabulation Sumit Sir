// Sumit Sir Solution
    int knapSack(vector<int>& val, vector<int>& wt, int cap) {
        int n = val.size();
        vector<int> dp(cap+1, 0);
        for(int i=1;i<=cap;i++){
            int curr_max = 0;
            for(int j=0;j<n;j++){
                if(i - wt[j] >= 0)
                {
                    dp[i] = max(dp[i], dp[i - wt[j]] + val[j]);
                }
            }
        }
        return dp[cap];
    }
