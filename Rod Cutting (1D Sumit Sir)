int cutRod(vector<int> &price) {
        int n = price.size();
        // vector<vector<int>> dp(n+1, vector<int> (n+1, 0));
        // vector<int> prev(n+1, 0);
        vector<int> dp(n+1, 0);
        for(int i=1;i<=n;i++){
            for(int j=1;j<=n;j++){
                if(i-j >= 0){
                    dp[i] = max(dp[i], price[j-1] + dp[i-j]);
                }
            }
        }
        return dp[n];
    }
